{
  "name": "digital-toolbox",
  "version": "1.0.0",
  "description": "A modern collection of programming tools with an intuitive web interface",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": ["web-scraping", "pdf-processing", "data-extraction", "excel-export"],
  "author": "Rob Salamon",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^2.1.4",
    "xlsx": "^0.18.5",
    "multer": "^1.4.5-lts.1",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}
